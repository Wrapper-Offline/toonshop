<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark">

	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>

	<s:Rect
		top="0" bottom="0" left="0" right="0"
		minWidth="14" minHeight="15">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:entries>
					<s:GradientEntry color="0xF2F2F2"/>
					<s:GradientEntry color="0xFAFAFA" ratio="0.75"/>
					<s:GradientEntry color="0xDFDFDF" ratio="1"/>
				</s:entries>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	<s:Line left="0" right="0" top="0">
		<s:stroke>
			<s:SolidColorStroke color="0xE4E4E4"/>
		</s:stroke>
	</s:Line>

</s:SparkSkin>