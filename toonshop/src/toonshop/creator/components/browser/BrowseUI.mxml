<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 implements="toonshop.creator.interfaces.IBrowseUIContainer">
	
	<fx:Script>
		<![CDATA[
			public function get ui_stockChars() : DataGroup
			{
				return this.stockChars;
			}
		]]>
	</fx:Script>
	
	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:SolidColor color="0xEEEEEE"/>
		</s:fill>
	</s:Rect>
	<s:DataGroup id="stockChars" itemRenderer="spark.skins.spark.DefaultComplexItemRenderer">
		<s:layout>
			<s:TileLayout
				horizontalGap="25"
				verticalGap="20"
				columnAlign="justifyUsingGap"
				paddingRight="12"
				paddingLeft="12"
				useVirtualLayout="true"
				orientation="rows"/>
		</s:layout>
	</s:DataGroup>
	
</s:Group>
